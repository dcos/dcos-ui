    "prebuild-ce": "./scripts/validate-tests",
    "build-ce": "npm run clean && npm test && npm run build",
    "serve": "NODE_ENV='development' npm run build && ./node_modules/.bin/gulp serve",
    "serve-notify": "export NOTIFY=true && npm run serve",
    "testing": "NODE_ENV='testing' npm run build && ./node_modules/.bin/gulp serve",
