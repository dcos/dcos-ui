  "scripts": {
    "analyzebundle": "NODE_ENV=production webpack --config ./webpack/webpack.analyze.js",
    "build:assets": "npm run clean && lingui compile && NODE_ENV=production webpack --colors --config ./webpack/webpack.production.js",
    "build:validate": "scripts/validate-build dist/assets/*.js",
    "build": "npm run build:assets && npm run build:validate",
    "build-release-bump": "./scripts/build-release-bump.sh",
    "clean": "rm -rf dist locale/_build",
    "lint:commits": "./node_modules/.bin/commitlint",
    "lint:styles": "bash -c 'opts=$@ ; set -f; if [[ $# -lt 1 ]]; then opts=\"{src/styles,plugins,packages}/**/*.less\"; fi ; stylelint ${opts}' 0",
    "lint:ts": "jest tslint",
    "lint:prettier": "pretty-quick --check",
    "lint": "npm run test:validate && npm run lint:prettier && npm run lint:styles && npm run lint:ts",
    "postbuild:assets": "find ./dist -name 'index.*.css' -exec node ./scripts/inline-bootstrap-css.js ./dist/index.html {} \\;",
    "postinstall": "npm run util:scaffold; npm run util:env:validate",
    "prebuild:assets": "node ./scripts/checkDependencies.js",
    "preinstall": "./scripts/pre-install",
    "prestart": "npm run util:env:validate",
    "pretty-quick": "pretty-quick",
    "release": "semantic-release",
    "start:testing": "NODE_ENV=testing webpack-dev-server --config ./webpack/webpack.dev.js --hot --progress --colors --content-base ./dist --host 0.0.0.0 --port $npm_package_config_port",
    "start": "lingui compile && NODE_ENV=development webpack-dev-server --config ./webpack/webpack.dev.js --hot --progress --colors --content-base ./dist --host 0.0.0.0 --port $npm_package_config_port",
    "test:integration:local": "cypress open",
    "test:integration": "./scripts/run-integration-tests",
    "test:system": "./scripts/run-system-tests",
    "test:system:local": "source scripts/utils/load_auth_env_vars; cypress open --config-file=cypress.system-tests.json",
    "test:validate": "./scripts/validate-tests",
    "test:watch": "npm test -- --watch",
    "test": "lingui compile && tsc > /dev/null; jest --no-cache",
    "typecheck": "jest typecheck",
    "updateTSSnapshots": "jest typecheck tslint -u",
    "util:env:validate": "node ./scripts/validate-engine-versions.js",
    "util:lingui:extract": "lingui extract --config ./linguirc",
    "util:lingui:check": "./scripts/lingui-ensure-extracted",
    "util:scaffold": "node ./scripts/ensureConfig.js",
    "versions": "echo npm: $(npm --version) && echo node: $(node --version)"
  },
  "jest-webpack-alias": {
    "configFile": "./webpack/webpack.dev.js"
  }
}
