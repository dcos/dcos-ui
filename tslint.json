{
  "extends": ["@dcos/tslint-config", "tslint-react-hooks", "tslint-config-prettier"],
  "rules": {
    "interface-name": [false],
    "interface-over-type-literal": false,
    "jsx-wrap-multiline": false,
    "no-submodule-imports": [false],
    "prefer-const": true,
    "react-hooks-nesting": "error"
  }
}
